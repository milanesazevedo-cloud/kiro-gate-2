# Scalingo Environment Variables Template
# Copy this file to .env.scalingo and fill in your values
# Then use: source .env.scalingo && ./deploy_scalingo.sh

# Scalingo API Token (keep this secure!)
export SCALINGO_API_TOKEN=your_scalingo_api_token_here

# Kiro Gateway Configuration
export PROXY_API_KEY=your_secure_proxy_api_key_here
export REFRESH_TOKEN1=your_first_account_refresh_token
export REFRESH_TOKEN2=your_second_account_refresh_token
export REFRESH_TOKEN3=your_third_account_refresh_token
export REFRESH_TOKEN4=your_fourth_account_refresh_token
export REFRESH_TOKEN5=your_fifth_account_refresh_token
export BACKGROUND_REFRESH_INTERVAL=600
export LOG_LEVEL=INFO

# Server Configuration
export SERVER_HOST=0.0.0.0
